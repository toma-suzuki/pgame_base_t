{
  "name": "pgame-base-typescript",
  "version": "2.3.0",
  "build-no": 66,
  "description": "pgame-base-typescript",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/before-build.js -m local && npm-run-all --parallel build-dev-watch start-dev-server",
    "build": "npm run build:pgame",
    "build:pgame": "npm-run-all build:pgame:dev build:pgame:prod",
    "build:pgame:dev": "node scripts/before-build.js -m dev && webpack --config webpack/webpack.config.dev.js",
    "build:pgame:prod": "node scripts/before-build.js -m prod && webpack --config webpack/webpack.config.prod.js",
    "build-dev-watch": "webpack --watch --config webpack/webpack.config.dev.js",
    "start-dev-server": "webpack-dev-server --inline --config webpack/webpack.config.dev.js --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "http://192.168.0.3/tenryu_test/pgame-base-typescript.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.10",
    "@types/createjs": "0.0.29",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.20.5",
    "@types/redux-logger": "^3.0.8",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "commander": "^7.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "eventemitter3": "^4.0.7",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "ts-loader": "^4.5.0",
    "typescript": "^3.9.9",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}
